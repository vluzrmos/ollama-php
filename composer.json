{
    "name": "vluzrrmos/ollama-php",
    "description": "Cliente PHP para a API do Ollama",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "vluzr",
            "email": "your-email@example.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
